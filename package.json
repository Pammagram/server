  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tsc --noEmit && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "jest -- src/modules",
    "test:integration": "npx dotenv -e .env.test jest -- tests/integration",
    "test:e2e": "npx dotenv -e .env.test jest --config ./src/tests/e2e/jest.e2e-config.ts -- e2e",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "postinstall": "husky install",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
